    <TIER ANNOTATOR="sidorP" DEFAULT_LOCALE="en"
        LINGUISTIC_TYPE_REF="translation" PARENT_REF="@avp1974m-text"
        PARTICIPANT="@avp1974m" TIER_ID="trans@avp1974m">
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a21" ANNOTATION_REF="a1">
                <ANNOTATION_VALUE>Cool! And this is how you rest in the evening here.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a22" ANNOTATION_REF="a4">
                <ANNOTATION_VALUE>How much land do you have, Illarion?</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a26" ANNOTATION_REF="a5">
                <ANNOTATION_VALUE>Five thousand hectares?</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a27" ANNOTATION_REF="a7">
                <ANNOTATION_VALUE>And, accordingly, also, corn and soy, yes?</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a28" ANNOTATION_REF="a8">
                <ANNOTATION_VALUE>Now the soybeans have been removed, the corn has been planted.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a29" ANNOTATION_REF="a10">
                <ANNOTATION_VALUE>And is the cattle dairy or for meat?</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a30" ANNOTATION_REF="a13">
                <ANNOTATION_VALUE>Yes.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a31" ANNOTATION_REF="a16">
                <ANNOTATION_VALUE>Well, in the village, you said, live almost a thousand people. </ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a32" ANNOTATION_REF="a19">
                <ANNOTATION_VALUE>Wow.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a44" ANNOTATION_REF="a41">
                <ANNOTATION_VALUE>And how many children do you have?</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a50" ANNOTATION_REF="a47">
                <ANNOTATION_VALUE>That's a lot, too!</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
    </TIER>
    <TIER ANNOTATOR="sidorP" DEFAULT_LOCALE="en"
        LINGUISTIC_TYPE_REF="translation" PARENT_REF="@obl1978m-text"
        PARTICIPANT="@obl1978m" TIER_ID="trans@obl1978m">
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a23" ANNOTATION_REF="a2">
                <ANNOTATION_VALUE>Yes, yes.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a24" ANNOTATION_REF="a3">
                <ANNOTATION_VALUE>It does not suit me, as they say, but  children.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a25" ANNOTATION_REF="a6">
                <ANNOTATION_VALUE>Well, today is not sowing and with livestock it is about five thousand, maybe, five thousand seven hundred, yes.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a33" ANNOTATION_REF="a9">
                <ANNOTATION_VALUE>Yes, yes, yes, well, I have both land for crops and stock, now we open more land, half of the land is used for crops, another half is used for stock, yes.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a34" ANNOTATION_REF="a11">
                <ANNOTATION_VALUE>No, for meat.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a35" ANNOTATION_REF="a12">
                <ANNOTATION_VALUE>We buy more cows already with calves.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a36" ANNOTATION_REF="a14">
                <ANNOTATION_VALUE>Then we sell some calves, some calves stay for a while and sell them when they grow up,</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a38" ANNOTATION_REF="a17">
                <ANNOTATION_VALUE>A lot.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a39" ANNOTATION_REF="a18">
                <ANNOTATION_VALUE>My mother has about ninety grandchildren.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a40" ANNOTATION_REF="a20">
                <ANNOTATION_VALUE>And if ten families like ours have 90 children each, can you imagine hom many of us it will be in total.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a45" ANNOTATION_REF="a42">
                <ANNOTATION_VALUE>We have...</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a46" ANNOTATION_REF="a43">
                <ANNOTATION_VALUE>Few.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <REF_ANNOTATION ANNOTATION_ID="a49" ANNOTATION_REF="a48">
                <ANNOTATION_VALUE>We only have four.</ANNOTATION_VALUE>
            </REF_ANNOTATION>
        </ANNOTATION>
    </TIER>
